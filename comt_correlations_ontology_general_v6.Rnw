     same.symbols <- match(hold.Lowest.pvals[[comp.indx[kl,1]]]$Symbol,hold.Lowest.pvals[[comp.indx[kl,2]]]$Symbol)

     tempa.indx <- which(!is.na(same.symbols))
     tempb.indx <- na.omit(same.symbols)
     
